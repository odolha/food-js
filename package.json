{
  "name": "@foodjs/main",
  "version": "0.0.1",
  "private": true,
  "watch": {
    "ui:scss": {
      "patterns": [
        "."
      ],
      "extensions": "scss",
      "ignore": "node_modules",
      "delay": 1000
    }
  },
  "scripts": {
    "init": "npm i -g rimraf",
    "clean": "rimraf node_modules && rimraf ./package-lock.json",
    "ui:scss": "node-sass ./ui/style/index.scss -o ./ui",
    "ui:sd": "node ./ui/live-server-start.js",
    "be:sd": "node --max_old_space_size=2000 --experimental-modules ./be/index.mjs",
    "be:sd-debug": "node %NODE_DEBUG_OPTION% --max_old_space_size=1000 --experimental-modules ./be/index.mjs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.2.0",
    "axios": "0.18.0",
    "bootstrap": "4.1.1",
    "buefy": "0.6.6",
    "concurrently": "4.0.1",
    "fs-extra": "6.0.1",
    "jquery": "3.3.1",
    "moment": "2.22.2",
    "opn": "^5.4.0",
    "popper.js": "1.14.4",
    "recursive-readdir-sync": "1.0.6",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2",
    "vue": "2.5.16",
    "vue-awesome-notifications": "2.2.9",
    "vue-nav-tabs": "0.5.7",
    "vue-router": "3.0.1",
    "vuejs-dialog": "1.3.0",
    "ws": "5.2.1",
    "yargs": "13.2.0"
  },
  "devDependencies": {
    "live-server": "1.2.0",
    "node-sass": "4.9.0",
    "npm-watch": "0.3.0"
  }
}
